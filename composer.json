{
    "name": "tuezy/laravel-tools",
    "description": "",
    "keywords": [
        "tuezy",
        "laravel-tools"
    ],
    "homepage": "https://github.com/tuezy/laravel-tools",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tuezy",
            "email": "tuezy@koding.men",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "v11.3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\Modules\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tuezy\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tuezy\\LaravelToolsServiceProvider",
                "Tuezy\\Core\\CoreServiceProvider"
            ],
            "aliases": {
                "LaravelTools": "Tuezy\\ToolFacade"
            }
        }
    }
}
